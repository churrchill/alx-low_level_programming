/**

 * _strncpy - function that copies a string

 * @dest: destination char

 * @src: source char

 * @n: number of bytes from src

 * Return: new string

 */



char *_strncpy(char *dest, char *src, int n)

{

int i;



for (i = 0; i < n && src[i] != '\0'; i++)

dest[i] = src[i];

for ( ; i < n; i++)

dest[i] = '\0';



return (dest);

}
