/**
* _strcat - gets the lenght
* @dest: a pointer parameter
* @src: a second pointer
* Return: void
*/
char *_strcat(char *dest, char *src)
{
int i = 0, n = 0;
while (*(a + n) != '\0')
{
n++;
}
while (*(b + i) != '\0')
{
*(a + i + n) =* ( b + i);
i++;
}
